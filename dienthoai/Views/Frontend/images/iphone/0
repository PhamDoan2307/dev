  <script>
  	$(function(){
    	$(".title-product-home a").click(function(){
    		var top = $(window).scrollTop();
    		$("html,body").animate({scrollTop:top+1},"fast");
    
    		if($(this).attr("href")!='#'){
              $(".title-product-home a").removeClass("current");
              $(this).addClass("current");
              
              $(".product-list.home").hide();
              $($(this).attr("href")).show();
    		}
    		return false;
    	});
    });
    
    $(function(){
    	$("#m-list-title-pro-home li a").click(function(){
    		
    		if($(this).attr("href")!='#'){
              $("#m-list-title-pro-home li a").removeClass("current");
              $(this).addClass("current");
              
              $(".product-list.home").hide();
              $($(this).attr("href")).show();
    		}
    		return false;
    	});
    });
    
    $(function(){
      $("#m-list-title-pro-home a").click(function(){
      	$(this).parent().children(".ul").toggle();
    	return false;
      });
    });
  </script>
