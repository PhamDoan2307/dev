    <script type="text/javascript">
  function subscribe_newsletter(){
      var email = document.getElementById('email_newsletter').value;
      var tel = document.getElementById('tel_newsletter').value;
  	  var pro = document.getElementById('pro_newsletter').value; 		
  
      if(email.length > 3){
        $.post("/ajax/register_for_newsletter.php", {email:email,full_name:tel, other : pro}, function(data) {
          if(data=='success') {
            alert("Quý khách đã đăng ký thành công "); 
          }else if(data=='exist'){ 
          	alert("Email này đã tồn tại");
          }else { 
          	alert('Lỗi xảy ra, vui lòng thử lại '); 
          }
        });
      }else{
        alert('Vui lòng nhập địa chỉ email');
      }  
  }
  
  function subscribe_newsletter2(){
      var email = document.getElementById('email_newsletter2').value;
      var tel = document.getElementById('tel_newsletter2').value;
  	  var pro = document.getElementById('pro_newsletter2').value; 		
  
      if(email.length > 3){
        $.post("/ajax/register_for_newsletter.php", {email:email,full_name:tel}, function(data) {
          if(data=='success') {
            alert("Quý khách đã đăng ký thành công "); 
          }else if(data=='exist'){ 
          	alert("Email này đã tồn tại");
          }else { 
          	alert('Lỗi xảy ra, vui lòng thử lại '); 
          }
        });
      }else{
        alert('Vui lòng nhập địa chỉ email');
      }  
  }

</script>
