  <script>
  	$(function(){
    	$("#mango-tv #related-link a").click(function(){
    		var top = $(window).scrollTop();
    		$("html,body").animate({scrollTop:top+1},"fast");
    
    		$("#mango-tv #related-link a").removeClass("current");
    		$(this).addClass("current");
    		
    		$("#mango-tv .row").hide();
    		$($(this).attr("href")).show();
    		return false;
    	});
    });
  </script>
