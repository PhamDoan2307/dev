<script>
  function formatCurrency(a) {
    var b = parseFloat(a).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1.").toString();
    var len = b.length;
    b = b.substring(0, len - 3);
    return b;
}
  
  //Menu mobile
  $("#m-menu-top").click(function(){
    $(this).toggleClass("active");
    $(this).find(".m-category.header").toggle();
  	$('body').toggleClass("fixed");
  	$(".bg_over_site").toggle();
  });
  
  
$(document).ready(function(){
  //Set link current
  $("a").each(function(){
  url = location.href.toLowerCase();
  if (this.href.toLowerCase() == url) {
  	$(this).addClass("current");
  }
  });
  
  //Show summary
  $(".item-pro").hover(function(){
  	$(this).find(".content-hover").stop(false,true).fadeIn();
  },function(){
  	$(this).find(".content-hover").stop(false,true).fadeOut();
  });
  
  $(".banner-item").hover(function(){
  	$(this).find(".banner-hover").stop(false,true).fadeIn();
  },function(){
  	$(this).find(".banner-hover").stop(false,true).fadeOut();
  });
});
</script>

